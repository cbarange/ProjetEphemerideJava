<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="1000.0" styleClass="background" stylesheets="@../../Foreground.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="foreground.liste.listeEvenement.ListeEvenementController">
   <children>
      <Pane fx:id="barrePane" prefHeight="60.0" prefWidth="1000.0" />
      <GridPane fx:id="gpe" layoutX="50.0" layoutY="150.0" prefHeight="100.0" prefWidth="900.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
      <Text fill="WHITE" layoutX="120.0" layoutY="120.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Liste des Evénements :">
         <font>
            <Font size="41.0" />
         </font>
      </Text>
      <Button layoutX="700.0" layoutY="80.0" mnemonicParsing="false" onAction="#btnAjouterEvenement" prefHeight="50.0" prefWidth="250.0" styleClass="bouton" text="Ajouter un événement">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Pane fx:id="barreOpenPane" prefHeight="500.0" prefWidth="1000.0" visible="false" />
   </children>
</AnchorPane>
